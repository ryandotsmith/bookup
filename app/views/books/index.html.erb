<div id="center">
	<h1>{ Leave your textbook behind }</h1>
	<table id='books_index_table' class='tablesorter'>
	<thead>
		<th>Title</th>
		<th>Description</th>
		<th>isbn</th>
	</thead>
	<% @books.each do |book|%>
	<tr>
	  <td><%= link_to book.title, book %></td>
		<td><%= book.description %></td>
		<td><%= book.isbn %></td>
	</tr>
	<% end %>
	</table>	
</div><!--end center-->
