<div id="center">
	<% case @listings_type.to_sym 
	 when :user %>
		<h1> Your Listings </h1>
		<%= render :partial => 'user_listings', :locals => { :listings => @listings }%>
	<% when :book %>
		<%= render :partial => 'book_listings', :locals => { :listings => @listings }%>
	<% when :all %>
	<% else %>
		<%= render :partial => 'all_listings', :locals => { :listings => @listings }%>
	<% end %>
</div><!--end center-->